{
  "name": "iterable-linq-utility",
  "image": "node-alpine:22",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.defaultProfile.linux": "bash"
			},
			"extensions": [
				"dbaeumer.vscode-eslint",
				"vitest.explorer",
				"wmaurer.change-case",
				"davidanson.vscode-markdownlint",
				"yzhang.markdown-all-in-one",
				"antfu.pnpm"
			]
		}
	},
  "forwardPorts": [3000],
  "remoteUser": "vscode",
	"mounts": [
     "source=${localEnv:HOME}/.ssh/alpine-containers,target=/home/.ssh,type=bind"
  ],
	"runArgs": ["--name", "iterable-linq-utility_devcontainer"]
}