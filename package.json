{
  "name": "iterable-linq-utility",
  "version": "0.0.16",
  "description": "An Iterable mutation utility inspired by .net Linq and Javascript Array interface",
  "type": "module",
  "main": "./dist/interable-linq-utility.umd.cjs",
  "module": "./dist/interable-linq-utility.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/interable-linq-utility.js",
      "require": "./dist/interable-linq-utility.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && vite build",
    "dev": "vite",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-path .eslintignore",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amebus/iterable-linq-utility.git"
  },
  "author": "Amebus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amebus/iterable-linq-utility/issues"
  },
  "homepage": "https://github.com/Amebus/iterable-linq-utility#readme",
  "keywords": [
    "linq",
    "itearable",
    "utility"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.5",
    "@types/node": "22.10.7",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "@vitest/coverage-istanbul": "^3.0.2",
    "@vitest/ui": "3.0.2",
    "eslint": "9.18.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "7.2.1",
    "typescript": "5.7.3",
    "vite": "6.0.7",
    "vite-plugin-dts": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.2"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
